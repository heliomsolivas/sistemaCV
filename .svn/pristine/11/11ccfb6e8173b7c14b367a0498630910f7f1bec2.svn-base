.swMain {
  position:relative;
  display:block;
  margin:0;
  padding:0;
  border: 0 solid #CCC;
  overflow:visible;
  float:left;
  width:600px;
}
.swMain .stepContainer {
  display:block;
  position: relative;
  margin: 0;
  padding:0;    
  border: 0 solid #CCC;
  clear:both;
  height:auto;
}

.swMain .stepContainer div.content {
  display:block;
  position: absolute;  
  float:left;
  margin-top: 10px;
  padding:5px;    
  border: 1px solid #CCC; 
  font: normal 12px Verdana, Arial, Helvetica, sans-serif; 
  color:#5A5655;   
  background-color:#F6F6F6;  
  height: auto;
  text-align:left;
  overflow:visible;    
  z-index:999; 
  -webkit-border-radius: 5px;
  -moz-border-radius  : 5px;
  width:600px;
  clear:both;
}

.swMain div.actionBar {
  display:block;
  position: relative; 
  clear:both;
  margin: 40px 0 0 0;
  padding: 0;
  color: #5A5655;
  height: auto;
  text-align: center;
  overflow:auto;    
  z-index:88;
  -webkit-border-radius: 5px;
  -moz-border-radius  : 5px;
  left:0;
  width: 615px;
}

.swMain .stepContainer .StepTitle {
  display:block;
  position: relative;
  margin:0;   
  border:1px solid #F6F6F6;
  padding:5px;   
  font: bold 16px Verdana, Arial, Helvetica, sans-serif; 
  color:#5A5655;   
  background-color:#E0E0E0;
  clear:both;
  text-align: center; 
  z-index:88;
  -webkit-border-radius: 5px;
  -moz-border-radius  : 5px;    
}
.swMain ul.anchor {
  position: relative;
  display:block;
  float:left;
  list-style: none;
  padding: 0;
  margin: 0px 0px;
  clear: both;
  border: 0 solid #CCCCCC;
  background: transparent; /*#EEEEEE */
}
.swMain ul.anchor li{ 
  position: relative; 
  display:block;
  margin: 0;
  padding: 0 3px;
  border: 0 solid #E0E0E0;
  float: left;
}
/* Anchor Element Style */
.swMain ul.anchor li a {
  display:block;
  position:relative;
  float:left;
  margin: 5px 0 0 0;
  padding:3px;
  height:30px;
  width:140px;
  text-decoration: none;
  outline-style:none;
  -moz-border-radius  : 5px;
  -webkit-border-radius: 5px;
  z-index:99;
}
.swMain ul.anchor li a .stepNumber{
  position:relative;
  float:left;
  width:20px;
  text-align: center;
  font: bold 15px Verdana, Arial, Helvetica, sans-serif;
}
.swMain ul.anchor li a .stepDesc{
  position:relative;
  display:block;
  float:left;
  text-align: left;
  padding:5px;
  font: bold 15px Verdana, Arial, Helvetica, sans-serif;
}
.swMain ul.anchor li a .stepDesc small{
  font: normal 12px Verdana, Arial, Helvetica, sans-serif;
}
.swMain ul.anchor li a.selected{
  color:#F6F6F6;
  background: #C2C2C2;  /* EA8511 */
  border: 1px solid #C2C2C2;
  cursor:text;
  border-radius: 4px 4px 4px 4px;
  -moz-box-shadow: 5px 5px 8px #888;
  -webkit-box-shadow: 5px 5px 8px #888;
  box-shadow: 5px 5px 8px #888;
}
.swMain ul.anchor li a.selected:hover {
  color:#000000;
  background: #C5C5C5;  
}
.swMain ul.anchor li a.done { 
  position:relative;
  color:#fff;  
  background: #8CC63F;  
  border: 0px solid #8CC63F;
  border-radius: 4px 4px 4px 4px;
  z-index:99;
}
.swMain ul.anchor li a.done:hover {
  color:#000000;  
  background: #C5C5C5; 
  border: 0px solid #5A5655;
  border-radius: 4px 4px 4px 4px;
}
.swMain ul.anchor li a.disabled {
  color:#fff;
  background: #dddddd;
  border: 0px solid #fff;  
  cursor:text;
  border-radius: 4px 4px 4px 4px;
}
.swMain ul.anchor li a.disabled:hover {
  color:#000000;  
  background: #dddddd;
  border: 1px solid #00000;
  border-radius: 4px 4px 4px 4px;
}

.swMain ul.anchor li a.error {
  color:#F6F6F6; !important;  
  background: #f08f75 !important;
  border: 0px solid #fb3500 !important;      
}
.swMain ul.anchor li a.error:hover {
  color:#000 !important;       
}

.swMain a:hover {
  color:#000000;  
  background: #C5C5C5;
  border: 1px solid #C5C5C5;
}
.swMain .buttonNext {
  font-family: inherit;
  display:block;
  float:right;
  margin: 5px;
  border-radius: 4px 4px 4px 4px;
  background-color: #F6F6F6;
  border: 1px solid #C5C5C5;
  color: #2E2E2E;
  padding:5px;
  text-decoration: none;
  text-align: center;
  width:100px;
  line-height: 1.66em;
  -moz-box-sizing: border-box;
}
.swMain .buttonDisabled {
  color: #2E2E2E;  !important;
  background-color: #E3E3E3!important;
  border: 1px solid #C5C5C5;
  cursor:text;    
}
.swMain .buttonPrevious {
  font-family: inherit;
  display:block;
  float:right;
  margin: 5px;
  border-radius: 4px 4px 4px 4px;
  background-color: #F6F6F6;
  border: 1px solid #C5C5C5;
  color: #2E2E2E;
  padding:5px;
  text-decoration: none;
  text-align: center;
  width:100px;
  line-height: 1.66em;
  -moz-box-sizing: border-box;   
}
.swMain .buttonFinish {
  font-family: inherit;
  display:block;
  float:right;
  margin: 5px;
  border-radius: 4px 4px 4px 4px;
  background-color: #F6F6F6;
  border: 1px solid #C5C5C5;
  color: #2E2E2E;
  padding:5px;
  text-decoration: none;
  text-align: center;
  width:100px;
  line-height: 1.66em;
  -moz-box-sizing: border-box;  
}

/* Form Styles */

.txtBox {
  border:1px solid #CCCCCC;
  color:#5A5655;
  font:13px Verdana,Arial,Helvetica,sans-serif;
  padding:2px;
  width:430px;
}
.txtBox:focus {
  border:1px solid #EA8511;
}

.swMain .loader {
  position:relative;  
  display:none;
  float:left;  
  margin: 2px 0 0 2px;
  padding:8px 10px 8px 40px;
  border: 1px solid #F6F6F6; 
  font: bold 13px Verdana, Arial, Helvetica, sans-serif; 
  color:#5A5655;       
  background: #FFF url(../images/loader.gif) no-repeat 5px;  
  -moz-border-radius  : 5px;
  -webkit-border-radius: 5px;
  z-index:998;
}
.swMain .msgBox {
  position:absolute;  
  display:none;
  margin: 0px 0 0 0px;
  padding: 3px;
  border: 1px solid #F6F6F6;
  background-color: #F6F6F6;
  color:#FF0000;
  -moz-border-radius  : 5px;
  -webkit-border-radius: 5px;
  border-radius: 4px 4px 4px 4px;
  z-index:999;
  width: 350px;
}
.swMain .msgBox .content {
  font: normal 12px Verdana,Arial,Helvetica,sans-serif;
  padding: 0;
  float:left;
}
.swMain .msgBox .close {
  border: 1px solid #CCC;
  border-radius: 3px;
  color: #CCC;
  display: block;
  float: right;
  margin: 0 0 0 5px;
  outline-style: none;
  padding: 0 2px 0 2px;
  position: relative;
  text-align: center;
  text-decoration: none;
}
.swMain .msgBox .close:hover{
  color:#FF0000;
  border: 1px solid #FF0000;
  background-color: #F6F6F6;
}