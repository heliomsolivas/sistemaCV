/*css global tabela*/
.full_table_list { 
    border-collapse: collapse;
    border: 2px solid #ffffff;
			padding: 0px 10px 0px 10px;
			border-collapse: collapse; 
}
/* CSS Tabelas       */
/*colocando bordas nao titulo da tabela*/
full_table_list td {
	border: 2px solid #ffffff;
}
/*colocando bordas nas celulas */
.full_table_list th {
	border: 2px solid #ffffff;
	color:#ffffff;
	background: #708090;
	border-radius: 8px;
}
/*Definido cor das linhas pares*/
.full_table_list tr: nth-child(even) {
	background: #F5F5F5;
}
/*Definindo cor das Linhas imp√°res*/
.full_table_list tr: nth-child(odd) {
	background: #DDDDDD;
}
