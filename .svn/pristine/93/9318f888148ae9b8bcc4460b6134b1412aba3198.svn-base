/*LAYOUT DAS PAGINAS*/
.divConteudo {
	height: 100%;
	position: absolute;
	top: 12px;
	width: 100%;
	z-index: 4;
}
.divConteudo #tituloPagina {
	color: #696969;
	font-family: Arial;
	font-size: 18px;
	font-weight: bold;
	height: 30px;
	margin-left: 10px;
	width: 100%;
}
.divConteudo #conteudoPagina {
	background-color: #F7F7F7;
	border: 3px solid #E3E3E3;
	height: 415px;
	margin: auto;
	padding: 10px 10px 10px 10px;
	width: 960px;
}
.divConteudo #conteudoPagina .conteudo {
	background-color: #F7F7F7;
	border: 2px solid #E6E6E6;
	height: 360px;
	padding: 10px 10px 10px 10px;
	width: 800px;
} 
.divConteudo #conteudoPagina .botoes {
	height: 24px;
	line-height: 24px;
	margin-top: 10px;
	width: 280px;
}
.divConteudo #conteudoPagina .mensagem {
	color: #ED3237;
	font-family: Arial;
	font-size: 11px;
	font-weight: bold;
	float: right;
	height: 30px;
	line-height: 30px;
	margin-top: -29px;
	margin-left: 285px;
	width: 675px;
}
.divConteudo #conteudoPrincipal {
	height: 415px;
	margin: auto;
	padding: 10px 10px 10px 10px;
	width: 960px;
}
.divConteudo #conteudoPrincipal .item {
	background-color: #F7F7F7;
	border: 3px solid #E3E3E3;
	padding: 10px 10px 10px 10px;
}
.divConteudo #conteudoPrincipal .item .imagemItem {
	display: block;
	width: 100%;
	text-align: center;
	margin-top: 5px;
}
.divConteudo #conteudoPrincipal .item .tituloItem {
	color: #696969;
	display: block;
	font-family: Arial;
	font-size: 14px;
	font-weight: bold;
	margin-top: 10px;
	width: 100%;
	text-align: center;
}
.divConteudo #conteudoPrincipal .item .nenhumResultado {
	color: #ED3237;
	font-family: Arial;
	font-size: 11px;
	font-weight: bold;
	line-height: 30px;
	margin-left: 10px;
}
.divConteudo #conteudoPrincipal .botoesPesquisa {
	height: 24px;
	line-height: 24px;
	margin-top: 10px;
	width: 135px;
}
.divConteudo #conteudoPrincipal .item .mensagemPesquisa {
	color: #ED3237;
	font-family: Arial;
	font-size: 11px;
	font-weight: bold;
	float: right;
	height: 30px;
	line-height: 30px;
	position: absolute;
	margin-top: -29px;
	margin-left: 140px;
	width: 648px;
}
.divConteudo #conteudoPrincipal .item .mensagem {
