	height: 500px;
	margin-top: 4px;
	position: relative;
}
#acoes  {
	float: left;
	width: 100%;
	background: #9AC0CD;
	height: 30px;
	margin-top: 4px;
	position: relative;
}
#footer  {
	float: left;
	background: #9AC0CD;
	height: 50px;
	margin-top: 4px;
