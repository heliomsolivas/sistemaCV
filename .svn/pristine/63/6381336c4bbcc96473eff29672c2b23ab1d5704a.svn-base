package reserva.bean;

public enum Situacao {

	APROVADA("Aprovada"),
	AGUARDANDO_RETORNO("Aguardando Retorno"),
	AGUARDANDO_PAGAMENTO("Aguardando Pagamento"),
	ENCERRADA("Encerrada"),
	CANCELADA("Cancelada");

	private String nome;

	private Situacao(String nome) {
		this.nome = nome;
	}

	public String getNome() {
		return nome;
	}
}