			function exibe(url, title, height, width, id) {
				if (document.getElementById(id) == null){
					var w = $("<div id='" + id + "'></div>").appendTo("#container");
					
					w.kendoWindow({
						minHeight: 0,
						minWidth: 300,
						maxHeight: 520,
						pinned: true,
						width: width,
						height: height,
					 	position: {
							top: 150,
							left: 480,
					  	},
						resizable: false,
						visible: false,
						content: url,
						title: title,
						actions: ["Minimize", "Close"],
						close: function() {
							w.data("kendoWindow").destroy();
						}
					});
					w.data("kendoWindow").open();
				} else {
					$("#" + id).data("kendoWindow").toFront();
					$("#" + id).data("kendoWindow").restore();
				}
			}
			
			function consultar(url, title, x, width, height, id) {
				if (document.getElementById(id) == null){
					var w = $("<div id='" + id + "'></div>").appendTo("#container");
					
					LeftPosition = (screen.width) ? (screen.width-width)/2 : 0;
					TopPosition = (screen.height) ? (screen.height-height)/2 : 0;
					
					w.kendoWindow({
						modal: true,
						height: x,
						width: width,
						position: {
							top: TopPosition,
							left: LeftPosition,
						},
						resizable: false,
						visible: false,
						content: url,
						title: title,
						actions: ["Close"],
						close: function() {
							w.data("kendoWindow").destroy();
						}
					});
					w.data("kendoWindow").open();
				} else {
					$("#" + id).data("kendoWindow").toFront();
					$("#" + id).data("kendoWindow").restore();
				}
			}
			
			function infoAcomodacao(url, title, id) {
				var w = $("<div id='" + id + "'></div>").appendTo("#container");
				w.kendoWindow({
					modal: true,
					height: "390px",
					position: {
						top: 180,
						left: 500,
					},
					resizable: false,
					visible: false,
					content: url,
					title: title,
					actions: ["Close"],
					close: function() {
						w.data("kendoWindow").destroy();
					}
				});
				w.data("kendoWindow").open();
			}

			function maisInfoAcomodacao () {
				var w = $("<div id='" + this.id + "'></div>").appendTo("#container");
				w.kendoWindow({
				  	modal: true,
				  	height: "390px",
				  	width: "400px",
                    position: {
			    		top: 200,
			    		left: 500,
				  	},
                    resizable: false,
                    visible: false,
                //    content: url,
                    actions: ["Close"],
                    close: function() {
						w.data("kendoWindow").destroy();
                    }
                });
                w.data("kendoWindow").open();
			}
			
			function excluir (url, urlRetorno, id) {
				var r = confirm("Deseja Excluir este item?");
				if(r == true) {
					$.ajax({
						contentType: 'application/x-www-form-urlencoded; charset=UTF-8',
						type: "POST",
						url: url,
						success: function(retorno) {
									$("#" + id).load(urlRetorno);
								 }
					});
				}
			}
			
			function checkOut () {
				var r = confirm("Deseja Finalizar esta Hospedagem?");
				if(r == true) {
					$.ajax({
						contentType: 'application/x-www-form-urlencoded; charset=UTF-8',
						type: "POST",
						url: "<c:url value="/clientes/checkOut"/>",
						data: $("#formulario").serialize(),
						success: function(retorno) {
									$("#conteudo").load("<c:url value="/sistema/divAcomodacoes"/>");
									$("#listaHospedagem").load("<c:url value="/clientes/listaHospedagem"/>");
									$("#listaAcomodacao").load("<c:url value="/hotel/listaAcomodacao"/>");
								}
					});
				}
			}
