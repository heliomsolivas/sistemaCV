<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<%@ page pageEncoding="UTF-8" contentType="text/html; charset=UTF-8" %>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %>
<%@ taglib uri="http://java.sun.com/jsp/jstl/fmt"  prefix="f" %>
<%@ taglib uri="http://displaytag.sf.net" prefix="display" %>

<html lang="pt-br">
	<head>
		<title>LedHotel</title>
		<meta http-equiv="Cache-control" content="no-cache">
		<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
		
		<script type="text/javascript">
			function voltar(){
				$("#reservaConteudo").load("<c:url value="/reserva/formulario"/>");
			}
		</script>
	</head>
	<body class="divConteudo">
		<div style="width: 480px; margin-top: 120px; margin-left: 80px; font-size: 24px; font-family: monospace; letter-spacing: 1px;">
			<div style="text-align: center;">
				Reserva <i> ${codigo} </i> Realizada com Sucesso!
			</div>
			<div style="text-align: center;">
				<span class="icon32 icon-check icon-darkgray"></span>
			</div>
			<div style="text-align: right; font-size: 12px; margin-top: 80px;">
				<a href="#" onclick="voltar();">
					<span class="icon icon-arrowthick-w icon-darkgray"></span> Voltar
				</a>
			</div>
		</div>
	</body>
</html>