			function sair() {
				$.confirm({
		            'title': '<f:message key="sair"/>',
		            'message': '<f:message key="mensagem.sair.sistema"/>',
		            'buttons': {
		                'Sim': {
		                    'class'	: 'confirm',
		                    'action': function(){
		                    	$("#conteudo").waitMe({effect :'ios', text :'', bg :'rgba(255,255,255,0.5)', color :'#000', sizeW :'', sizeH :''});
		                    	window.location = "<c:url value="/sistema/logout"/>";
	                    	}
		                },
		                'Não': {
		                    'class'	: 'cancel',
		                    'action': function(){}
		                }
		            }
		        });
			}
